<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AptInfoController">
   <children>
      <GridPane hgap="10.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="218.0" minWidth="10.0" prefWidth="104.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="361.0" minWidth="10.0" prefWidth="346.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="AptInfoDate" text="Date:" />
            <Label fx:id="AptInfoStart" text="Start Time:" GridPane.rowIndex="1" />
            <Label fx:id="AptInfoEnd" text="End Time:" GridPane.rowIndex="2" />
            <Label fx:id="AptInfoTitle" text="Title:" GridPane.rowIndex="3" />
            <Label fx:id="AptInfoCustomer" text="Customer:" GridPane.rowIndex="4" />
            <Label fx:id="AptInfoContact" text="Contact:" GridPane.rowIndex="5" />
            <Label fx:id="AptInfoType" text="Type:" GridPane.rowIndex="6" />
            <Label fx:id="AptInfoDescription" text="Description:" GridPane.rowIndex="7" />
            <Label fx:id="AptInfoLocation" text="Location:" GridPane.rowIndex="8" />
            <Label fx:id="AptInfoDateLbl" text="Label" GridPane.columnIndex="1" />
            <Label fx:id="AptInfoStartLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="AptInfoEndLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="AptInfoTitleLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="AptInfoCustLbl" onMouseClicked="#aptInfoExpandCustomer" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="AptInfoContactLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label fx:id="AptInfoTypeLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label fx:id="AptInfoDescLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Label fx:id="AptInfoLocLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="8" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="450.0">
         <children>
            <Button fx:id="AptInfoBackBtn" mnemonicParsing="false" onAction="#AptInfoBack" text="Back" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
