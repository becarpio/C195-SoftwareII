<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="375.0" prefWidth="609.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustListController">
   <children>
      <TableView fx:id="CustListTbl" prefHeight="300.0" prefWidth="550.0">
        <columns>
          <TableColumn fx:id="CustListNameCol" prefWidth="213.0" text="Name" />
          <TableColumn fx:id="CustListPhoneCol" prefWidth="203.0" text="Phone Number" />
            <TableColumn fx:id="CustListCityCol" minWidth="0.0" prefWidth="172.0" text="City" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="25.0" prefWidth="600.0" spacing="20.0">
         <children>
            <Button fx:id="CustListMoreInfoBtn" mnemonicParsing="false" onAction="#CustListMoreInfo" text="More Info" />
            <Button fx:id="CustListAddBtn" mnemonicParsing="false" onAction="#CustListAdd" text="Add" />
            <Button fx:id="CustListModBtn" mnemonicParsing="false" onAction="#CustListMod" text="Modify" />
            <Button fx:id="CustListDelBtn" mnemonicParsing="false" onAction="#CustListDel" text="Delete" />
            <Button fx:id="CustListBackBtn" mnemonicParsing="false" onAction="#CustListBack" text="Back" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
