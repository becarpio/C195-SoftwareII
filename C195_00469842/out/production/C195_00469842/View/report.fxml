<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="250.0" prefWidth="300.0" spacing="15.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ReportController">
   <children>
      <Button fx:id="ReportCustAptBtn" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#ReportCustApt" text="Apt by Customer" />
      <Button fx:id="ReportMonAptBtn" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#ReportMonApt" text="Apt by Month" />
      <Button fx:id="ReportConsultSchBtn" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#ReportConsultSch" text="Consultant Schedule" />
      <Button fx:id="ReportGenerateAllBtn" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#ReportGenerateAll" text="Generate All" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="ReportBackBtn" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#ReportBack" text="Back" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
   </padding>
</VBox>
